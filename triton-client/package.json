{
	"name": "triton-client",
	"version": "0.1.0",
	"private": true,
	"description": "Triton frontend client",
	"license": "LGPLv3",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/c3g/triton.git"
	},
	"bugs": {
		"url": "https://github.com/c3g/triton/issues"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			".eslintrc.json"
		],
		"overrides": [
			{
				"files": [
					"**/*.stories.*"
				],
				"rules": {
					"import/no-anonymous-default-export": "off"
				}
			}
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"antd": "^4.23.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"redux-logger": "^3.0.6"
	},
	"devDependencies": {
		"@reduxjs/toolkit": "^1.8.3",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.3.0",
		"@types/jest": "^27.5.2",
		"@types/node": "^17.0.45",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@types/redux-logger": "^3.0.9",
		"node-sass": "^7.0.3",
		"react-scripts": "5.0.1",
		"sass": "^1.54.3",
		"typescript": "^4.7.4",
		"webpack": "^5.88.2"
	}
}
