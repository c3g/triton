{
	"name": "triton-types",
	"version": "1.0.0",
	"description": "Type definition library for shared client/server types",
	"main": "src/index.ts",
	"types": "models/index.d.ts",
	"scripts": {
		"build": "tsc",
		"start": "npx nodemon",
		"test": "echo \"Error: no test specified\" && exit 1"
