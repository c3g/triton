		"exec": "npm run build"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard-with-typescript": "^23.0.0",
		"nodemon": "^3.0.1",
		"prettier": "^2.7.1",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"kysely": "^0.26.3"
	}
}
